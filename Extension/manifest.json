{
    "name": "Save Tabs",
    "version": "1.0",
    "description": "Save open tabs for later use",
    "permissions": ["tabs", "storage"],
    "browser_action": {
        "default_title": "Save pages",
        "default_icon": "favicon.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }